                        <tr print-spacer="Tier 1"></tr>
                        <tr print-tracks="{tier:1,cat:'petrol-falls'}"></tr>
                        <tr print-spacer="Tier 2"></tr>
                        <tr print-tracks="{tier:2,cat:'petrol-falls'}"></tr>
                        <tr print-spacer="Tier 3"></tr>
                        <tr print-tracks="{tier:3,cat:'petrol-falls'}"></tr>

                    </table>
                </div>
            </div>
        {{/with}}
    {{/content}}
{{/extend}}