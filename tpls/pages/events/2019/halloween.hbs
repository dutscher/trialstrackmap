{{#extend "default"}}
    {{#content "title-override"}}true{{/content}}
    {{#content "title" mode="prepend"}}
        Halloween 2019 -
    {{/content}}
    {{#content "styles" mode="append"}}
        <style>
            body {
                padding: 0;
            }

            * {
                box-sizing: border-box;
            }

            .fb {
                background: #fadda1;
                padding: 6px;
                border-radius: 8px;
            }

            .hunt-container {
                display: flex;
                flex-direction: column;
                color: #fadda1;
                text-shadow: -1px -1px 0 #443925,
                1px -1px 0 #443925,
                -1px 1px 0 #443925,
                1px 1px 0 #443925;
                padding: 8px;
            }

            @media (min-width:992px) {
                .hunt-container {
                    flex-direction: row;
                }
            }

            h1 {
                color: #fadda1;
                margin: 8px;
                margin-right: 16px;
                line-height: 12px;
                padding: 24px 0;
            }

            h1 span {
                font-size: 12px;
                color: #EC7000;
            }

            table {
                margin: 8px;
                border-spacing: 0;
            }

            tr {
                position: relative;
            }

            tr:hover {
                color: #EC7000;
            }

            td {
                min-width: 10px;
                white-space: nowrap;
                vertical-align: middle;
                padding: 0 6px;
            }

            td i {
                width: 15px !important;
                height: 15px !important;;
            }

            i.medal-platinum {
                background: url(//i.imgur.com/jAFrKkL.png);
            }

            i.medal-gold {
                background: url(//i.imgur.com/E9nANVv.png);
            }

            i.medal-silver {
                background: url(//i.imgur.com/OJmAWbw.png);
            }

            i.faults {
                background: url(//i.imgur.com/ZDhEx6w.png);
            }

            td a {
                display: inline-block;
                margin-top: 6px;
                border: solid 3px #fadda1;
                cursor: pointer;
            }

            td a:hover {
                border-color: #EC7000;
            }

            td img {
                display: block;
                width: 30px;
                height: 30px;
            }

            .cell.cell--letter {
                padding: 8px;
            }

            .hunt-container .medals {
                display: none;
                font-size: 12px;
            }

            .trackenv {
                background-size: cover;
                background-position-y: center;
            }

            {{#with ../../gfx}}
                {{#each images.envs}}
                .trackenv--{{@key}} {
                    background-image: url({{hoster this ../hoster}});
                }
                {{/each}}
            {{/with}}

            .modal {
                display: none;
                position: relative;
                z-index: 1337;
                text-shadow: -1px -1px 0 #443925,
                1px -1px 0 #443925,
                -1px 1px 0 #443925,
                1px 1px 0 #443925;
                color: #fadda1;
            }

            .modal.modal--is-open {
                display: flex;
                position: fixed;
                height: 100vh;
                width: 100vw;
                align-items: center;
                justify-content: center;
            }

            .modal::after {
                content: '';
                background: #000;
                opacity: 0.75;
                position: absolute;
                height: 100vh;
                width: 100vw;
                z-index: -1;
            }

            .modal__content {
                position: relative;
                display: flex;
                align-items: center;
            }

            .modal__content::before {
                content: attr(data-trackname);
                position: absolute;
                top: -40px;
                color: #fadda1;
                width: 100%;
                text-align: center;
                font-size: 25px;
            }

            .modal .img {
                display: block;
                margin: 4px;
                box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.75);
                width: 10vw;
            }

            .modal .img--is-hidden {
                visibility: hidden;
            }

            .modal .img--actual {
                width: 50vw;
            }

            .modal .img__actual__wrap {
                position: relative;
            }

            .modal .img--is-loading::before {
                content: '';
                width: 100%;
                height: 100%;
                position: absolute;
                z-index: 1337;
                left: 0;
                top: 0;
                background: #000;
                opacity: 0.75;
            }

            .modal .img--is-loading::after {
                content: '';
                background-size: cover !important;
                width: 60px;
                height: 60px;
                position: absolute;
                z-index: 1337;
                left: 50%;
                top: 50%;
                margin-top: -30px;
                margin-left: -30px;
                background-image: url('data:image/png;base64,');
                animation: clock-spin 1.5s infinite ease-out;
            }

            .modal .img__wrap {
                position: relative;
                cursor: pointer;
            }

            .modal .img__wrap::before {
                content: attr(data-index);
                position: absolute;
                width: 100%;
                text-align: center;
                font-size: 45px;
                top: 50%;
                transform: translateY(-50%);
            }

            .modal .medals {
                position: absolute;
                bottom: -51px;
                display: inline-flex;
                left: 50%;
                align-items: center;
                transform: translateX(-50%);
            }
        </style>
        <script>
            var openModal, clickOutside, imgLoaded;
            (function () {
                var activeElement = null;
                var modalSelector = '.modal';
                var medalSelector = '.medals';
                var contentSelector = '.modal__content';
                var isOpen = 'modal--is-open';
                var isHidden = 'img--is-hidden';
                var isLoading = 'img--is-loading';
                var modal;
                var actualIndex;

                openModal = function (element, direction) {
                    if (direction) {
                        if (direction === 'previous' && activeElement.previousElementSibling) {
                            activeElement = activeElement.previousElementSibling;
                        } else if (direction === 'next' && activeElement.nextElementSibling) {
                            activeElement = activeElement.nextElementSibling;
                        }
                    } else {
                        activeElement = element;
                    }

                    modal = document.querySelector(modalSelector);
                    var medals = modal.querySelector(medalSelector);
                    var content = modal.querySelector(contentSelector);
                    var actual = modal.querySelector('.img--actual');
                    actualIndex = parseInt(activeElement.getAttribute('data-index')) + 1;

                    medals.innerHTML = activeElement.parentNode.parentNode.querySelector(medalSelector).innerHTML;
                    content.setAttribute('data-trackname', activeElement.parentNode.getAttribute('data-trackname'));
                    actual.setAttribute('src', activeElement.getAttribute('data-img'));
                    actual.parentNode.setAttribute('href', activeElement.getAttribute('data-img'));
                    // show spinner
                    imgLoaded(actual, true);

                    handleNavi(-1);
                    handleNavi(1);

                    // show modal
                    modal.classList.add(isOpen);
                };

                clickOutside = function (modal, event) {
                    if (event.target.classList.contains('img__wrap') || event.target.classList.contains('img')) {
                        return false;
                    }
                    var modal = document.querySelector(modalSelector);
                    // close modal
                    modal.classList.remove(isOpen);
                }

                function handleNavi(direction) {
                    var directionStr = (direction === 1 ? 'next' : 'previous');
                    var navi = modal.querySelector('.img--' + directionStr);
                    var hasNavi = activeElement[directionStr + 'ElementSibling'];
                    // disable
                    if (!hasNavi) {
                        navi.parentNode.classList.add(isHidden);
                    // show thumb
                    } else {
                        navi.parentNode.setAttribute('data-index', (actualIndex + direction) + ".");
                        navi.setAttribute('src', hasNavi.getAttribute('data-thumb'));
                        navi.parentNode.classList.remove(isHidden);
                    }
                }

                imgLoaded = function (img, add) {
                    img.parentNode.parentNode.classList[!add ? 'remove' : 'add'](isLoading)
                }
            })();
        </script>
    {{/content}}

    {{#content "body"}}
        <div class="modal" onclick="clickOutside(this, event)">
            <div class="modal__content">
                <div class="medals"></div>
                <div class="img__wrap img--is-hidden" onclick="openModal(this, 'previous')">
                    <img class="img img--previous"/>
                </div>
                <div class="img__actual__wrap img--is-loading">
                    <a href="#" target="_blank">
                        <img class="img img--actual" onload="imgLoaded(this)"/>
                    </a>
                </div>
                <div class="img__wrap img--is-hidden" onclick="openModal(this, 'next')">
                    <img class="img img--next"/>
                </div>
            </div>
        </div>
        {{#with ../../hunt-events.database.events.hunt.halloween_2019}}
            <div class="hunt-container">
                <h1>
                    Halloween Heat 2019<br/>
                    <span>
                        {{#hunt-event-stats huntData=this}}
                            Tracks: {{tracks}} /
                        Locations: {{locations}}
                        {{/hunt-event-stats}}
                    </span>
                </h1>
                {{> facebook-script}}
                <div class="fb">
                    {{> facebook-div url="https://www.facebook.com/trialstrackmap" width=250}}
                </div>
            </div>
            <div class="hunt-container">
                {{#ranges '["A-G","H-R","S-Z"]'}}
                    <table>
                        <thead>
                            <th width="50%"></th>
                        </thead>
                        {{#hunt-event-sorter
                                tracksData=../this
                                chunks=range
                                medalFile="database/trackdata/times.json"
                                i18nFile="database/i18n/en.json"}}
                            {{#if renderLetter}}
                                <tr>
                                    <td colspan="3" class="cell cell--letter">
                                        {{letter}}
                                    </td>
                                </tr>
                            {{/if}}
                            {{#hunt-event
                                    huntData=huntData
                                    id=trackId
                                    trackIdsFile="database/trackdata/ids.json"
                            }}
                                <tr class="trackenv trackenv--{{trackEnv}}">
                                    <td>
                                        {{../trackName}}
                                    </td>
                                    <td data-locations="{{locations}}" data-trackname="{{../trackName}}">
                                        {{#each images}}
                                            <a onclick="openModal(this)"
                                               target="_blank"
                                               data-index="{{@index}}"
                                               data-img="{{image}}"
                                               data-thumb="{{thumb}}"
                                               title="{{description}} found by {{founder}}">
                                                <img src="{{thumb}}"/>
                                            </a>
                                        {{/each}}
                                    </td>
                                    <td>
                                        <span class="medals">
                                            {{hunt-event-medals ../medals}}
                                        </span>
                                    </td>
                                </tr>
                            {{/hunt-event}}
                        {{/hunt-event-sorter}}
                    </table>
                {{/ranges}}
            </div>
        {{/with}}
    {{/content}}
{{/extend}}