<title page-title="{{#block "title-override"}}{{/block}}" data-update="{{#with map}}{{version}}{{/with}}">
    {{#block "title"}}{{title}}{{/block}}
</title>
<meta name="description" content="{{#block "description"}}{{description}}{{/block}}"/>

<meta name="copyright" content="Â© {{year}} dutscher-DE"/>
<meta http-equiv="Access-Control-Allow-Origin" content="*">
